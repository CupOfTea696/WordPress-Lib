{
    "name": "cupoftea/wordpress-lib",
    "description": "WordPress Library",
    "authors": [
        {
            "name": "Sven Wittevrongel",
            "email": "sven@cupoftea.io"
        }
    ],
    "license": "MIT",
    "require": {
        "cupoftea/counter": "^1.1",
        "cupoftea/package": "^1.3",
        "illuminate/config": "5.2.*|5.3.*",
        "illuminate/container": "5.2.*|5.3.*",
        "illuminate/filesystem": "5.2.*|5.3.*",
        "illuminate/support": "5.2.*|5.3.*",
        "illuminate/view": "5.2.*|5.3.*",
        "monolog/monolog": "^1.19",
        "symfony/debug": "^3.0",
        "symfony/http-foundation": "^3.0",
        "symfony/var-dumper": "^3.0",
        "vlucas/phpdotenv": "^2.2"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "CupOfTea\\WordPress\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    }
}
